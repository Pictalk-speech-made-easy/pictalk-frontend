{
  "name": "Pictalk AAC",
  "version": "2.6.9",
  "description": "Pictalk is an app which aims to make speech easier for non-verbal people. Pictalk builds sentences with pictograms you uploaded and pronounces them out loud. You can build virtually any sentence and speak any language ! ",
  "author": "Alexandros SIDIRAS GALANTE",
  "private": true,
  "scripts": {
    "dev": "nuxt -H 0.0.0.0",
    "build": "nuxt build",
    "start": "nuxt start",
    "start:cypress": "npm run start & cypress run",
    "generate:start": "API_URL=https://api.pictalk.org npm run generate && npm run start",
    "generate": "nuxt generate",
    "analyze": "nuxt build --analyze",
    "cypress:open": "cypress open"
  },
  "devDependencies": {
    "@nuxtjs/i18n": "^7.2.0",
    "@nuxtjs/pwa": "^3.3.5",
    "@nuxtjs/robots": "^2.5.0",
    "@nuxtjs/sentry": "^5.1.7",
    "@nuxtjs/sitemap": "^2.4.0",
    "axios": "^0.21.1",
    "babel-eslint": "^10.1.0",
    "cypress": "^12.3.0",
    "cypress-nuxt": "^1.3.1",
    "cypress-vue-unit-test": "^3.5.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.2.0",
    "jpegasus": "^1.16.1",
    "js-cookie": "^3.0.1",
    "merge-images-horizontally-with-text": "^1.0.2",
    "node-sass": "^6.0.1",
    "nuxt": "^2.15.8",
    "nuxt-buefy": "^0.4.22",
    "nuxt-clipboard2": "^0.2.1",
    "nuxt-compress": "^5.0.0",
    "nuxt-responsive-loader": "^2.0.1",
    "prettier": "^2.5.1",
    "responsive-loader": "^2.3.0",
    "sass-loader": "^10.1.1",
    "secure-ls": "^1.2.6",
    "sharp": "^0.29.3",
    "vuex-persistedstate": "^4.1.0",
    "nuxt-matomo": "^1.2.4"
  }
}
